[ircbot]
nick=<%= @nick %>
pass=<%= @password %>
server=<%= @server %>
port=6697
channel_config=/etc/gerritbot/channel_config.yaml
lockfile=/var/run/gerritbot/gerritbot.pid
log_config=/etc/gerritbot/logging.config

[gerrit]
user=<%= @user %>
key=/home/gerrit2/.ssh/gerritbot_rsa
host=<%= @vhost_name %>
port=29418
